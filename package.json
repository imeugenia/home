{
  "name": "myPage",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@size-limit/preset-app": "^4.9.1",
    "react-scripts": "4.0.1",
    "size-limit": "^4.9.1"
  },
  "engines": {
    "node": ">= 14.5.0"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "contentful": "^4.6.1",
    "lodash": "^4.17.20",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-icons": "^4.1.0",
    "react-native-vector-icons": "^7.1.0",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5",
    "redux-promise": "^0.6.0"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "size": "npm run build && size-limit"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "size-limit": [
    {
      "path": "build/static/js/main*.js",
      "limit": "170 KB"
    }
  ]
}
